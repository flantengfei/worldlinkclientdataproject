{
  "manifest_version": 2,

  "name": "chrome find amazon customer id extension",
  "description": "This extension will find customer ID for user",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["https://www.amazon.com/gp/profile/*"],
      "js": ["jquery.js", "content.js"],
      "run_at": "document_end"
    }
  ],

  "browser_action": {
   "default_icon": "icon.png"
  },
  "permissions": [
   "activeTab"
   ]
}
